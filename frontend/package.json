{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "cross-env NODE_OPTIONS='--max-old-space-size=2048' vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch --threshold warning --diagnostic-sources js,svelte",
		"lint": "eslint src",
		"lint:fix": "eslint src --fix",
		"format": "prettier --write src",
		"format:check": "prettier --check src"
	},
	"devDependencies": {
		"@eslint/js": "^10.0.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/node": "^25.3.0",
		"autoprefixer": "^10.4.24",
		"cross-env": "^10.1.0",
		"eslint": "^10.0.1",
		"eslint-plugin-svelte": "^3.15.0",
		"eslint-plugin-tailwindcss": "^3.18.2",
		"globals": "^17.3.0",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"rollup-plugin-visualizer": "^7.0.0",
		"svelte": "^5.53.2",
		"svelte-check": "^4.4.3",
		"svelte-eslint-parser": "^1.4.1",
		"tailwindcss": "^3.4.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.0",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"heic2any": "^0.0.4",
		"idb": "^8.0.3",
		"lucide-svelte": "^0.575.0",
		"qr-scanner": "^1.4.2",
		"rehype-external-links": "^3.0.0",
		"rehype-sanitize": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"unified": "^11.0.5"
	},
	"overrides": {
		"cookie": "^0.7.0",
		"minimatch": ">=10.2.1"
	}
}
